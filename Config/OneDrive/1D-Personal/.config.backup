# Configuration for OneDrive Linux Client
# This file contains the list of supported configuration fields
# with their default values.
# All values need to be enclosed in quotes
# When changing a config option below, remove the '#' from the start of the line
# For explanations of all config options below see docs/USAGE.md or the man page.
#
sync_dir = "~/0/OneDrive"
# sync_dir = "~/OneDrive"
skip_file = "~*|.~*|*.tmp"
skip_file = "desktop.ini"
skip_file = "Personal Vault.lnk"
# skip_file = "~*|.~*|*.tmp"
# monitor_interval = "300"
skip_dir = "Apps|Attachments|Desktop|Documents|Office Online Extension|local|Pictures"
skip_dir = ".obsidian|.cache"
skip_dir = "com-gmail-dqp|edu-princeton-dq"
# log_dir = "/var/log/onedrive/"
log_dir = "/var/log/onedrive/1D-Personal/"
# drive_id = ""
# upload_only = "false"
# check_nomount = "false"
# check_nosync = "false"
# download_only = "false"
# disable_notifications = "false"
# disable_upload_validation = "false"
# enable_logging = "false"
# force_http_11 = "false"
# local_first = "false"
# no_remote_delete = "false"
# skip_symlinks = "false"
# debug_https = "false"
# skip_dotfiles = "false"
# skip_size = "1000"
# dry_run = "false"
# min_notify_changes = "5"
# monitor_log_frequency = "6"
# monitor_fullscan_frequency = "12"
# sync_root_files = "false"
# classify_as_big_delete = "1000"
# user_agent = ""
# remove_source_files = "false"
# skip_dir_strict_match = "false"
# application_id = ""
# resync = "false"
# resync_auth = "false"
# bypass_data_preservation = "false"
# azure_ad_endpoint = ""
